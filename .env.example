# Telegram Bot
TELEGRAM_TOKEN=your_bot_token_here

# Database
DB_NAME=gis
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_HOST=localhost
DB_PORT=5432

# Connection Pool
